other {
    B:AllowVanillaShields=true
    B:FOV=true
    B:integration=true

    # When set flase, vanilla shield recipe is disabled forcing player to use this mod's shields
    B:mujmajnkraftsbettersurvival.configgui.allow_vanilla_shields=true

    # When set to true, your movement speed won't effect your field of view.
    B:mujmajnkraftsbettersurvival.configgui.fov=true

    # When set to true, you can craft weapons from materials from other mods
    B:mujmajnkraftsbettersurvival.configgui.integration=true
}


